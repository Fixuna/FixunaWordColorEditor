set "CompleteCode=%ESC%%colorCombination% %UserInput1% %colorReset%"
echo.
echo.
echo %ESC%[40;92mO============================================================================================================================================O%colorReset%
echo.
echo.
echo                                       The Program has used ANSI escape codes to color your word..
echo.
echo.
echo                                           %ESC%%colorCombination% %UserInput1% %colorReset%
echo.
echo.
echo %ESC%[40;96mOooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooO%colorReset%
echo.
echo.
echo Press R^) To Redirect your chosen word and color code to a text file for you to use in batch script.
echo Press M^) To Start the Color Menu from start and to continue with setting a word and color.
echo Press E^) To Exit the Program
echo.
set /p "Answer2=Enter your choice: "
if /i "%Answer2%" == "" goto WordColor
if /i %Answer2% == R goto Redirect1
if /i %Answer2% == M goto Menu1
if /i %Answer2% == E goto Exit
goto WordColor

:Redirect1
echo %ESC%[40;91mOo==========================================================================================================================================oO%colorReset%
echo.
echo.
set /p "Redirect=Do you want to redirect you complete code with commands to a text file^? ^(Yes/No^): "
if /i "%Redirect%" == "" goto Redirect1
if /i %Redirect% == No goto WordColor
if /i %Redirect% == Yes goto Redirect2
goto Redirect1

:Redirect2
echo.
echo %ESC%[40;91mOo==========================================================================================================================================oO%colorReset%
echo.
echo %CompleteCode% is the code...
(
    setlocal enabledelayedexpansion
	set com1="for ^/F "
	set com2="^"tokens^=^1^,^2 delims^=^#^" "
	set com3="%%%a in "
	set com4="^('"prompt ^#^$H^#^$E^# ^& echo on ^& for %%%b in ^(^1^) do rem"'^) "
	set com5="do ^( "
	set com6="set ESC^="
	set com7="%%%b"
	echo ----------------------------------------------------------------------------------- >> "%USERPROFILE%\Desktop\FixunaExe1\FolderColorOutput\OutputColorCodes.txt"
	echo. >> "%USERPROFILE%\Desktop\FixunaExe1\FolderColorOutput\OutputColorCodes.txt"
    echo %DATE% >> "%USERPROFILE%\Desktop\FixunaExe1\FolderColorOutput\OutputColorCodes.txt"
	echo %TIME% >> "%USERPROFILE%\Desktop\FixunaExe1\FolderColorOutput\OutputColorCodes.txt"
	echo ^<ESC^>!colorCombination!!UserInput1!^<ESC^>[0m >> "%USERPROFILE%\Desktop\FixunaExe1\FolderColorOutput\OutputColorCodes.txt"
	echo This is !displayText! >> "%USERPROFILE%\Desktop\FixunaExe1\FolderColorOutput\OutputColorCodes.txt"
	echo. >> "%USERPROFILE%\Desktop\FixunaExe1\FolderColorOutput\OutputColorCodes.txt"
	echo Call ^:setESC and Use the following code block in your Batch Script when you are having trouble with the ANSI ESC character. >> "%USERPROFILE%\Desktop\FixunaExe1\FolderColorOutput\OutputColorCodes.txt"
	echo And replace the ^<ESC^> for %% ESC %% without the spaces between percent characters around ESC in your own Batch Script^! >> "%USERPROFILE%\Desktop\FixunaExe1\FolderColorOutput\OutputColorCodes.txt"
	echo. >> "%USERPROFILE%\Desktop\FixunaExe1\FolderColorOutput\OutputColorCodes.txt"
	echo ----------------------------------------------------------------------------------- >> "%USERPROFILE%\Desktop\FixunaExe1\FolderColorOutput\OutputColorCodes.txt"
    endlocal
)
echo.
echo The Complete Code has been redirected to "OutputColorCodes.txt".
echo.
echo.
echo Continue to next option.. 
echo.
echo.
echo Press M^) To Start the Color Menu from start and to continue with setting a word and color.
echo Press E^) To Exit the Program
echo.
set /p "Answer2=Enter your choice: "
if /i "%Answer2%" == "" goto Redirect2
if /i %Answer2% == M goto Menu1
if /i %Answer2% == E goto Exit 
goto Redirect2

:Message1
echo %ESC%%colorBorder4%Ooo---------------------------------------------------------------------------------------------------------------------------------ooO%colorReset%
echo.
echo                             %ESC%[45;33moYou can not go further Back.. please ENTER a valid option..%colorReset%
echo.
pause
goto ColorMenu1

:Message2
echo %ESC%%colorBorder4%Ooo---------------------------------------------------------------------------------------------------------------------------------ooO%colorReset%
echo.
echo                             %ESC%[45;33moYou can not go further Forward.. please ENTER a valid option..%colorReset%
echo.
pause
goto ColorMenu8

:setESC
for /F "tokens=1,2 delims=#" %%a in ('"prompt #$H#$E# & echo on & for %%b in (1) do rem"') do (
  set ESC=%%b
  exit /B 0
)

exit /B 0

:Exit
cls
echo.
echo.
echo.
echo %ESC%[101;93mOo==========================================================================================================================================oO%colorReset%
echo.
echo.
echo                            %ESC%[102;93mThank you for using this program %UserName%^! Hope to see you soon^! Bye bye...%colorReset%
echo.
echo.
echo %ESC%[101;93mOo==========================================================================================================================================oO%colorReset%
echo.
echo.
echo.
timeout /t 3 >nul & exit
