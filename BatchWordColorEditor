@echo off
Title Fixuna Word Color Editer
color 0A
setlocal enabledelayedexpansion
cls
call :setESC
call :SetESCcodes
goto CheckInstall

:SetESCcodes
set "colorReset=%ESC%[0m"
set "colorBorder0=%ESC%[102;97m"
set "colorBorder1=%ESC%[102;95m"
set "colorBorder2=%ESC%[107;36m"
set "colorBorder3=%ESC%[101;35m"
set "colorBorder4=%ESC%[40;92m"
set "colorBorder5=%ESC%[105;93m"

:CheckInstall
echo.
echo.
echo %ESC%%colorBorder0%---------------------  Check Installation of Fixuna^'s Word Color Editor..  ---------------------%colorReset%
echo.
CD "%USERPROFILE%\DESKTOP\"
echo Program is in this directory:        %CD% 
echo User Profile is:        %USERPROFILE%
echo.
pause
echo.
echo Check if Folder exists..
echo.
pause
goto Check1

:Check1
if exist "%USERPROFILE%\DESKTOP\FixunaExe1\FolderColorOutput" ( 
	echo Folder exists && goto Main
) else (
 echo Folder not created && goto CreateFolder1
)

:CreateFolder1
echo.
echo %CD%
echo Press ENTER to create Folder..
echo.
pause >nul
CD "%USERPROFILE%\DESKTOP"
MD "%USERPROFILE%\DESKTOP\FixunaExe1"
MD "%USERPROFILE%\DESKTOP\FixunaExe1\FolderColorOutput"
echo.
echo Installation has been completed..
echo Folder and Output Text File has been made..
echo.
echo Press ENTER to Continue to use Fixuna's Word Color Editer..
echo.
pause 
goto Main

:Main
color 0F
cls
echo.
echo.
echo What is your Name user^?
set /p UserName=Enter Name:
pause
cls
echo.
echo.
echo ^*%ESC%%colorBorder0%===================================================================================================================================%colorReset%^*
echo.
echo.
echo.
echo                                                             ^<O %ESC%[45;33mHello %UserName%...%colorReset% O^>
echo.
echo.
echo                                               ^<O %ESC%[105;93mWelcome To Fixuna's Word Color Editor%colorReset% O^>
echo.
echo.
echo ^*%ESC%%colorBorder0%===================================================================================================================================%colorReset%^*
echo.
echo.
echo Press S^) to start the Color Menu
echo Press E^) to exit the Program
set /p "Answer1=Enter your choice: "
if /i "%Answer1%" == "" goto Main
if /i %Answer1% == S goto Start
if /i %Answer1% == E goto Exit 
goto Main

:Start
cls
color 0F
echo.
echo.
echo.
echo %ESC%%colorBorder1%=======================================================================================================================================%colorReset%
echo.
echo.
echo.
echo                                     %ESC%[101;93mWelcome to my program to use ANSI ESCAPE CODES for your Batch Script.%colorReset%
echo.
echo.
echo                              %ESC%[40;97mENTER your chosen Word or Words and look for the background and foreground color you like.%colorReset%
echo                                      %ESC%[40;97mYou can redirect the code so you can use it in your own Batch Script.%colorReset%
echo.
echo.
echo                                     %ESC%[40;93mI hope this program will help you to create beautiful Batch Scripts.%colorReset%
echo.
echo.
echo %ESC%%colorBorder1%=======================================================================================================================================%colorReset%
pause
goto Menu1

:Menu1
cls
color 0F
echo.
echo.
echo %ESC%%colorBorder2%=======================================================================================================================================%colorReset%
echo.
echo.
echo                                        %ESC%[40;97mEnter the word you want to color...%colorReset%
echo.
echo.
echo                      %ESC%[40;96m--------------------------------------------------------------------------%colorReset%
echo.
echo.
set /p UserInput1=Enter:
echo You word^(s^) is^/are.. %UserInput1%
echo.
echo.
goto Menu2

:Menu2
echo %ESC%%colorBorder2%=======================================================================================================================================%colorReset%
echo.
echo.
echo                                        %ESC%[40;97mDo you want to continue or change your Word...%colorReset%
echo.
echo                      %ESC%[40;96m--------------------------------------------------------------------------%colorReset%
echo.
echo.
set /p "ContinueWord=Do you want to continue with this Word^? ENTER ^(Yes/No^): "
if /i "%ContinueWord%" == "" goto Menu2
if /i %ContinueWord% == No goto Menu1
if /i %ContinueWord% == Yes goto Menu3
goto Menu2

:Menu3
cls
color 0F
echo.
echo.
echo.
echo %ESC%%colorBorder3%=======================================================================================================================================%colorReset%
echo %ESC%%colorBorder4%---------------------------------------------------------------------------------------------------------------------------------------%colorReset%
